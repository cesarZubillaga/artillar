html(ng-app="app")
    head
        title= 'Artillar Testing Site'
        script(src='/angular/angular.min.js')
        script(src='/chart.js/dist/Chart.min.js')
        script(src='/angular-chart.js/dist/angular-chart.min.js')
        script(src='/app/app.js')
        link(rel="stylesheet", type="text/css", href='/bootstrap/dist/css/bootstrap.min.css')
        link(href="https://fonts.googleapis.com/css?family=Ubuntu", rel="stylesheet")
        link(href="/css/style.css", rel="stylesheet")
    body(class="container-fluid")
        div(class="row")
         div(class="col-xs-12")
          h1(class="text-uppercase")
           i(class="logo glyphicon glyphicon-signal")
           |Artillar Testing
        nav(class="navbar navbar-default navbar-default-fixed-top")
         div(class="container")
          ul(class="nav navbar-nav")
           li
            a(href="/")="Data"
           li
            a(href="/runnabletests")="Runable Tests"
        block content